{
  "templates": {
    "applicationName": "ZangoDB",
    "disqus": "",
    "googleAnalytics": "",
    "openGraph": {
        "title": "",
        "type": "website",
        "image": "",
        "site_name": "",
        "url": ""
    },
    "meta": {
        "title": "",
        "description": "",
        "keyword": ""
    },
    "linenums": true
  },
  "source": {
      "include": [
          "./src/"
      ],
      "includePattern": ".+\\.js(doc)?$",
      "excludePattern": "(^|\\/|\\\\)_"
  },
  "opts": {
      "encoding": "utf8",
      "recurse": true,
      "private": false,
      "lenient": true,
      "destination": "./docs",
      "template": "./node_modules/jaguarjs-jsdoc"
  }
}
